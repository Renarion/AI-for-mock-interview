# Copy to backend/.env and keep secrets out of git

# Database connection (Docker: hostname "db")
DATABASE_URL=postgresql+asyncpg://postgres:postgres@db:5432/mock_interview

# JWT secret (required for auth)
SECRET_KEY=change-me-in-production

# Frontend URL (used for CORS)
FRONTEND_URL=https://analyticsinterview.live

# DEBUG=false in production
DEBUG=false

# LLM (see KEYS_SETUP_GUIDE.md)
OPENAI_API_KEY=
LLM_PROVIDER=openai

# YooKassa (see KEYS_SETUP_GUIDE.md)
YOOKASSA_SHOP_ID=
YOOKASSA_SECRET_KEY=
